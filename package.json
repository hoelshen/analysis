{
  "name": "analysis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run serve\" \"npm run dev:server\""
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-stage-1": "^7.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-meta": "^2.2.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-stage-1": "^7.0.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "koa": "^2.8.1",
    "koa-router": "^7.4.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
